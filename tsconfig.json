{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "target": "ES2021",
    "module": "commonjs",
    "rootDir": "./src",
    "outDir": "./compile",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "sourceMap": false,
    "types": [
      "node",
    ],
    "lib": [
      "ES2021"
    ],
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
  },
  "exclude": [
    "./src/test"
  ]
}